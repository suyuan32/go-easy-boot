syntax = "v1"

info(
	title: "captcha api"
	desc: "captcha api"
	author: "ryansu"
	email: "yuansu.china.work@gmail.com"
	version: "v1.0"
)

import "base.api"

type CaptchaInfoResp {
	BaseMsg
	Data CaptchaInfo `json:"data"`
}

type CaptchaInfo {
	CaptchaId string `json:"captchaId"`
	ImgPath   string `json:"imgPath"`
}

@server(
	group: captcha
	middleware: Cros
)

service system {
	@handler getCaptchaHandler
	get /captcha returns (CaptchaInfo)
}