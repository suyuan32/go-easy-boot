syntax = "v1"

info(
	title: "captcha api"
	desc: "captcha api"
	author: "ryansu"
	email: "yuansu.china.work@gmail.com"
	version: "v1.0"
)

import "base.api"

type CaptchaInfo {
	BaseMsg
	CaptchaId uint64 `json:"captchaId"`
	ImgPath   string `json:"imgPath"`
}

@server(
	group: captcha
)

service system {
	@handler getCaptchaHandler
	get /captcha returns (CaptchaInfo)
}